/* ==================
         消息组件
 ==================== */
.diy-notice-bar {
  padding: 18rpx 24rpx;
  overflow: hidden;
  width: 100%;
}
.diy-direction-row {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.diy-left-icon {
  align-items: center;
}
.diy-notice-box {
  flex: 1;
  display: flex;
  flex-direction: row;
  overflow: hidden;
  margin-left: 12rpx;
}
.diy-right-icon {
  margin-left: 12rpx;
  display: inline-flex;
  align-items: center;
}
.diy-notice-content {
  -webkit-animation: diy-loop-animation 10s linear infinite both;
          animation: diy-loop-animation 10s linear infinite both;
  text-align: right;
  padding-left: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.diy-notice-img {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
